{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "id",
      "series_short_name",
      "series_id",
      "short_name",
      "sortBy",
      "name",
      "logo"
    ],
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid"
      },
      "series_short_name": {
        "type": "string"
      },
      "series_id": {
        "type": "string",
        "format": "uuid"
      },
      "short_name": {
        "type": "string"
      },
      "sortBy": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "logo": {
        "type": "string",
        "format": "uri"
      }
    }
  }
}
